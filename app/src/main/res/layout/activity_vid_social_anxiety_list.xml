<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vid_social_anxiety_list">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="424dp"
        android:layout_height="0dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="47dp"
        android:src="@drawable/oh_no"
        app:layout_constraintBottom_toTopOf="@+id/socialV1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <Button
        android:id="@+id/socialV1"
        android:layout_width="126dp"
        android:layout_height="61dp"
        android:layout_marginBottom="63dp"
        android:text="Video 1"
        app:layout_constraintBottom_toTopOf="@+id/socialV2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView20" />

    <Button
        android:id="@+id/socialV2"
        android:layout_width="126dp"
        android:layout_height="61dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="103dp"
        android:text="Video 2"
        app:layout_constraintBottom_toTopOf="@+id/socialV3"
        app:layout_constraintStart_toStartOf="@+id/socialV1"
        app:layout_constraintTop_toBottomOf="@+id/socialV1" />


    <Button
        android:id="@+id/socialV3"
        android:layout_width="126dp"
        android:layout_height="61dp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="183dp"
        android:text="Video 3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/socialV2"
        app:layout_constraintTop_toBottomOf="@+id/socialV2" />

    <Button
        android:id="@+id/socialV4"
        android:layout_width="126dp"
        android:layout_height="61dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="80dp"
        android:text="Video 4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/socialV3"
        app:layout_constraintTop_toBottomOf="@+id/socialV3"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>